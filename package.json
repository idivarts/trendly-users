{
  "name": "trendly-users",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "eas-build-pre-install": "./eas-prebuild.sh",
    "ios": "expo run:ios",
    "ios-device": "expo run:ios --device",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build": "expo export -p web",
    "preview": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile preview --local --output build/Trendly.tar.gz && tar -xzf build/Trendly.tar.gz -C build && xcrun simctl install booted build/Trendly.app",
    "simulator": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile simulator --local --output build/Trendly.tar.gz && tar -xzf build/Trendly.tar.gz -C build && xcrun simctl install booted build/Trendly.app",
    "emulator": "export $(cat .env.local | xargs) &&  eas build --platform android --profile simulator --local --output build/Trendly.apk && adb install build/Trendly.apk",
    "build-ios": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile production --local --output build/Trendly.ipa",
    "build-android": "export $(cat .env.local | xargs) &&  eas build --platform android --profile production --local --output build/Trendly.aab",
    "submit-ios": "eas submit -p ios --path build/Trendly.ipa",
    "submit-android": "eas submit -p android --path build/Trendly.aab",
    "postinstall": "npm install --prefix shared-libs && npm install --prefix shared-uis && npm install --prefix shared-constants && node react-native-web-fix.js",
    "populate": "ts-node scripts/populate.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
