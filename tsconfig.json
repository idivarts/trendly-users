{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ],
      "*": [
        "./node_modules/*",
      ]
    },
    "moduleResolution": "node",
    "customConditions": null
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}