{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
   "exclude": [
    "./shared-libs/node_modules/*",
    "./shared-uis/node_modules/*",
    "./shared-constants/node_modules/*"
  ]
}